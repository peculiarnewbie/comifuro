                    "text": {
                      "type": "string"
                    },
                    "imageMask": {
                      "type": "number"
                    }
                  },
                  "required": [
                    "id",
                    "user",
                    "timestamp",
                    "text",
                    "imageMask"
                  ]
                }
              }
            },
            "multipart/form-data": {
              "schema": {
                "type": "array",
