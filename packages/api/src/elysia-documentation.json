                        },
                        {
                          "type": "string"
                        }
                      ]
                    },
                    "text": {
                      "type": "string"
                    },
                    "imageMask": {
                      "type": "number"
                    }
                  },
                  "required": [
                    "id",
                    "user",
                    "timestamp",
                    "text",
                    "imageMask"
                  ]
